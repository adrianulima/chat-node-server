{
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "prettier/standard",
    "plugin:jsdoc/recommended"
  ],
  "plugins": [
    "eslint-plugin-prettier",
    "jsdoc"
  ],
  "rules": {
    "prettier/prettier": "error",
    "comma-dangle": 0,
    "prefer-promise-reject-errors": 0,
    "jsdoc/require-param-description": 0,
    "jsdoc/require-returns-description": 0
  }
}
